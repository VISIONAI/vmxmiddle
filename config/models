User
    ident Text
    password Text Maybe
    UniqueUser ident
    deriving Typeable
Email
    email Text
    user UserId Maybe
    verkey Text Maybe
    UniqueEmail email

Model json
    user UserId Maybe
    uuid Text
    name Text
    size [Int] default="0 | 0"
    pos Int default=0
    neg Int default=0
    start UTCTime default=LOCALTIMESTAMP
    end   UTCTime default=LOCALTIMESTAMP
    image Text default='missing.jpg'
    UniqueModel uuid
    deriving Show Typeable

 -- By default this file is used in Model.hs (which is imported by Foundation.hs)
